{
  "name": "parcel-test",
  "version": "1.0.0",
  "description": "testing parcel build process with react, sass, express",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist",
    "dev:client": "parcel watch client/index.html --out-dir dist/client",
    "dev:server": "nodemon --watch ./server --exec babel-node ./server/index.js",
    "build:client": "parcel build client/index.html --out-dir dist/client",
    "build:server": "rollup -c",
    "prebuild": "rm -rf dist",
    "build": "npm run build:client && npm run build:server",
    "build:start": "npm run build && npm start",
    "dev": "npm run dev:server & npm run dev:client",
    "sandbox": "nodemon --watch ./client --exec babel-node ./client/state/demo.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "app-root-path": "^2.0.1",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.2",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "humanize-duration": "^3.14.0",
    "immutable": "^3.8.2",
    "react-dom": "^16.4.0",
    "redux-automap": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.5",
    "parcel": "^1.8.1",
    "react": "^16.4.0",
    "react-hot-loader": "^4.2.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-immutable": "^4.0.0",
    "reselect": "^3.0.1",
    "rollup": "^0.59.4",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "semantic-ui-css": "^2.3.1",
    "semantic-ui-react": "^0.80.2"
  }
}
